#!/usr/bin/expect -f

set username [lindex $argv 0]
set password [lindex $argv 1]
set remote_name [lindex $argv 2]
set token_key [lindex $argv 3]
set timeout 2

spawn git push $remote_name master

expect "Username"
send $username\r

expect {
    "token" {
        send $token_key\r
    }
    "Password" {
    	send $password\r
    }
}

interact